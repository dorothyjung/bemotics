<!DOCTYPE html>
<html>
<head>
  <% include ../partials/header.ejs %>
</head>

<body>

<% include ../partials/nav.ejs %>

<div class="container">
<h2>Headset data for test_user_1</h2>

<table class="table table-condensed">
  <tr>
    <td>Time (ms)</td>
    <td>Engagement</td> 
    <td>Frustration</td>
    <td>Short-Term Excitement</td>
    <td>Long-Term Excitement</td> 
  </tr>

  <% results.forEach(function(r) { %>
  	<tr>
			<td><%= r.time %></td>
			<td><%= r.engagement %></td>
			<td><%= r.frustration %></td>
			<td><%= r.shorttermexcitement %></td>
			<td><%= r.longtermexcitement %></td>
  	</tr>
	<% }); %>
  
</table>

</div>

  <!-- Footer -->
  <footer>
    <% include ../partials/footer.ejs %>
  </footer>

</body>
</html>
